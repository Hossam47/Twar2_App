    </style>

    <style name="checkBoxStyle" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorAccent">@color/white</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="colorControlNormal">@color/white</item>
        <item name="colorControlActivated">@color/white</item>
        <item name="colorControlHighlight">@color/white</item>
    </style>

    <style name="checkBoxStyle_terms" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorAccent">@color/white_trans</item>
        <item name="android:textColorHint">@color/white_trans</item>
        <item name="colorControlNormal">@color/white_trans</item>
        <item name="colorControlActivated">@color/white</item>
        <item name="colorControlHighlight">@color/white_trans</item>
    </style>

    <style name="checkBoxStyle_update" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorAccent">@color/primary_trans</item>
        <item name="android:textColorHint">@color/primary_trans</item>
        <item name="colorControlNormal">@color/primary_trans</item>
        <item name="colorControlActivated">@color/primary</item>
        <item name="colorControlHighlight">@color/primary_trans</item>
    </style>

    <style name="checkBoxStyle_intro" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorAccent">@color/intro_color1</item>
